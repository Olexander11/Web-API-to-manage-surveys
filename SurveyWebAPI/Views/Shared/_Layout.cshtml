<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <script src="http://ajax.microsoft.com/ajax/jquery/jquery-1.4.2.js" type="text/javascript"></script>
    <style>
        h2 {
            text-align: center;
        }
        .maintext {
            font-weight: bold;
            font-size: 18px;
        }
        .descrtext {
                font-style: italic;
                font-size: 18px;
        }
        .footer {
            position: fixed;
            left: 0;
            bottom: 0;
            width: 100%;
            background-color: white;
            color: darkblue;
            text-align: center;
        }
        div .table-border {
            border: 2px solid;
        }

        }
        .question {
            position: relative;
        }
             

        table {
            width: 100%;
            border-collapse: collapse;
        }
    </style>

</head>

    <body>
        <div class="header">
            <div>
                  <p>@Html.ActionLink("Home", "Index")</p>
            </div>
        </div>

        <div>
            @RenderBody()
        </div>

        <footer class="footer">
            <p>© 2019 - KPMG StudyProject</p>
        </footer>
    </body>



</html>
