
@{
    ViewBag.Title = "Create Survey";
}

<h2>Create new Survey</h2>
<form method="post" action="/Home/SubmitCreateSur">
    <table>
        <tr>
            <td><p>Input name of new survey :</p></td>
            <td><input type="text" name="name" size="120" /> </td>
        </tr>
        <tr>
            <td><p>Input description of new survey :</p></td>
            <td><input type="text" name="description"   size="120"/> </td>
        </tr>
        <tr>
            <td><input type="submit" value="Create" /> </td>
            <td></td>
        </tr>
    </table>
</form>